{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "react/prop-types": 0,
    "react/prefer-es6-class": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4]
  }
}
